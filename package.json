    "@types/react-table": "6.7.11",
    "@types/reactstrap": "6.0.0",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "gh-pages": "1.2.0",
    "react-scripts": "1.1.4",
    "react-test-renderer": "16.4.2",
    "redux-devtools": "3.4.1",
    "typescript": "3.0.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/data/*.{ts,tsx}",
      "!src/registerServiceWorker.ts",
      "!/node_modules/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 45,
