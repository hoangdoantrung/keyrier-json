    "react": "16.4.2",
    "react-ace": "6.1.4",
    "react-dom": "16.4.2",
    "react-redux": "5.0.7",
    "react-scripts-ts": "2.16.0",
    "react-select": "2.0.0",
    "react-table": "6.8.6",
    "reactstrap": "6.3.1",
    "redux": "4.0.0",
    "xlsx": "0.13.3"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
